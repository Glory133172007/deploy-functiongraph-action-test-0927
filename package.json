{
  "name": "deploy-functiongraph-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/*.{ts,tsx}\"",
    "format-check": "prettier --check \"src/*.{ts,tsx}\"",
    "package": "ncc build --source-map --license licenses.txt",
    "lint": "eslint src  --ext .ts",
    "all": "npm run lint && npm run build && npm run format && npm run package",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huaweicloud/functiongraph-deploy-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/io": "^1.1.2",
    "@actions/exec": "^1.1.1",
    "@huaweicloud/huaweicloud-sdk-core": "^3.0.46-rc",
    "@huaweicloud/huaweicloud-sdk-devstar": "^3.0.46-rc",
    "@huaweicloud/huaweicloud-sdk-functiongraph": "^3.0.46-rc",
    "@types/fs-extra": "^9.0.13",
    "fs-extra": "^10.0.1",
    "mime": "^3.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "typescript-action": "*"
  },
  "devDependencies": {
    "@cloud/eslint-config-cbc": "^1.7.2",
    "@types/jest": "^28.1.6",
    "@types/mime": "^2.0.3",
    "@types/node": "^16.10.5",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vercel/ncc": "^0.31.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-bevry": "^3.27.0",
    "eslint-plugin-mocha": "^9.0.0",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.5",
    "typescript": "^4.4.4"
  }
}
